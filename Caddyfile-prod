lobster.eecs.umich.edu

encode gzip

route {

    route /api/* {
        reverse_proxy lobster-backend:3000
    }

    route /public_api/* {
        reverse_proxy lobster-backend:3000
    }

    route /auth/* {
        reverse_proxy lobster-backend:3000
    }

    file_server {
        root /public
        index index.html
    }
}
